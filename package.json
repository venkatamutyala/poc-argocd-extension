{
  "name": "argocd-extension-glueops",
  "version": "1.0.0",
  "description": "ArgoCD UI extension for GlueOps platform integration",
  "main": "dist/extension.js",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "clean": "rm -rf dist",
    "package": "npm run clean && npm run build && tar -czf extension.tar.gz dist/ backend/"
  },
  "keywords": [
    "argocd",
    "extension",
    "glueops"
  ],
  "author": "GlueOps",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "babel-loader": "^9.1.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  }
}
